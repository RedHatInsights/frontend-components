{
  "name": "@redhat-cloud-services/rule-components",
  "version": "4.0.24",
  "description": "Components to be used when showing rule information",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "./**/*.css",
    "./**/*.scss"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedHatInsights/frontend-components.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/RedHatInsights/frontend-components/issues"
  },
  "homepage": "https://github.com/RedHatInsights/frontend-components/tree/master/packages/rule-componenets#readme",
  "dependencies": {
    "@patternfly/react-core": "^6.0.0",
    "@patternfly/react-icons": "^6.0.0",
    "@patternfly/react-table": "^6.0.0",
    "@redhat-cloud-services/frontend-components": "^7.0.13",
    "@redhat-cloud-services/frontend-components-utilities": "^7.0.0",
    "dot": "1.1.3",
    "react-dom": "^18.2.0",
    "marked": "^14.1.2",
    "sanitize-html": "^2.13.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./RuleTable": {
      "types": "./dist/RuleTable/index.d.ts",
      "import": "./dist/esm/RuleTable/index.js",
      "require": "./dist/RuleTable/index.js"
    },
    "./RuleFilters": {
      "types": "./dist/RuleFilters/index.d.ts",
      "import": "./dist/esm/RuleFilters/index.js",
      "require": "./dist/RuleFilters/index.js"
    },
    "./ReportDetails": {
      "types": "./dist/ReportDetails/index.d.ts",
      "import": "./dist/esm/ReportDetails/index.js",
      "require": "./dist/ReportDetails/index.js"
    },
    "./Markdown": {
      "types": "./dist/Markdown/index.d.ts",
      "import": "./dist/esm/Markdown/index.js",
      "require": "./dist/Markdown/index.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  }
}
