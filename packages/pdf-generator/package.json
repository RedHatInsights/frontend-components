{
    "name": "@redhat-cloud-services/frontend-components-pdf-generator",
    "version": "4.0.0",
    "description": "Common components for RedHat Cloud Services project to generate PDF from JS.",
    "browser": "dist/js/index.js",
    "module": "dist/esm/index.js",
    "sideEffects": [
        "./**/*.css",
        "./**/*.scss"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup -c ./rollup.config.js",
        "start": "rollup -c ./rollup.config.js -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/RedHatInsights/frontend-components.git"
    },
    "bugs": {
        "url": "https://github.com/RedHatInsights/frontend-components/issues"
    },
    "homepage": "https://github.com/RedHatInsights/frontend-components/tree/master/packages/pdf-generator#readme",
    "keywords": [
        "pdf",
        "generator",
        "jspdf",
        "pdfjs"
    ],
    "author": "Red Hat",
    "license": "Apache-2.0",
    "dependencies": {
        "@patternfly/react-charts": "7.1.1-prerelease.10",
        "@patternfly/react-core": "5.1.1-prerelease.23",
        "@patternfly/react-icons": "5.1.1-prerelease.8",
        "@patternfly/react-styles": "5.1.1-prerelease.8",
        "@patternfly/react-tokens": "5.1.1-prerelease.8",
        "@react-pdf/renderer": "^1.6.8",
        "@redhat-cloud-services/frontend-components": "^4.0.0",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "rgb-hex": "^3.0.0",
        "@scalprum/react-core": "^0.5.1"
    },
    "devDependencies": {
        "style-inject": "0.3.0",
        "rollup": "^2.46.0",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-terser": "^7.0.2"
    }
}
