{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:next": "next build",
    "build": "npm run generate:components:docs && next build",
    "start": "npm run build && next start -H 0.0.0.0 -p ${PORT:-8080}",
    "watch": "concurrently \"npm run generate:components -- --watch\" \"node generators/docs-generator.js --watch\" \"npm run generate:navigation -- --watch\" \"npm run dev\"",
    "generate:navigation": "node generators/generate-left-nav-common.js",
    "generate:components": "node generators/docgen.js",
    "generate:components:docs": "npm run generate:components && node generators/docs-generator.js && npm run generate:navigation",
    "generate:watch": "concurrently \"npm run generate:components -- --watch\" \"node generators/docs-generator.js --watch\" \"npm run generate:navigation -- --watch\"",
    "serve": "node server.js",
    "typedoc": "typedoc"
  },
  "dependencies": {
    "@patternfly/react-core": "4.192.0",
    "@patternfly/react-icons": "^4.7.18",
    "@patternfly/react-table": "^4.19.24",
    "@redhat-cloud-services/frontend-components": "^3.0.10",
    "classnames": "^2.3.1",
    "next": "^12.1.0",
    "prism-react-renderer": "^1.1.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-json-view": "^1.19.1",
    "react-jss": "^10.7.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^11.0.1",
    "codesandbox": "^2.2.1",
    "concurrently": "^5.3.0",
    "eslint-config-next": "^11.0.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "jsdoc": "^3.6.6",
    "jsdoc3-parser": "^2.0.0",
    "next-transpile-modules": "^8.0.0",
    "raw-loader": "^4.0.2",
    "react-docgen": "^5.3.1",
    "typedoc": "^0.22.10",
    "url-loader": "^4.1.1"
  }
}
