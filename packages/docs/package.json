{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:next": "next build",
    "build": "npm run generate:components:docs && next build",
    "start": "npm run build && next start -H 0.0.0.0 -p ${PORT:-8080}",
    "watch": "concurrently \"npm run generate:components -- --watch\" \"node generators/docs-generator.js --watch\" \"npm run generate:navigation -- --watch\" \"npm run dev\"",
    "generate:navigation": "node generators/generate-left-nav-common.js",
    "generate:components": "node generators/docgen.js",
    "generate:components:docs": "npm run generate:components && node generators/docs-generator.js && npm run generate:navigation",
    "generate:watch": "concurrently \"npm run generate:components -- --watch\" \"node generators/docs-generator.js --watch\" \"npm run generate:navigation -- --watch\"",
    "serve": "node server.js"
  },
  "dependencies": {
    "@mdx-js/react": "^2.1.3",
    "@mdx-js/mdx": "^2.1.3",
    "@patternfly/react-core": "^5.0.0",
    "@patternfly/react-icons": "^5.0.0",
    "@patternfly/react-table": "^5.0.0",
    "@redhat-cloud-services/frontend-components": "^4.0.0-beta.3",
    "classnames": "^2.3.2",
    "next": "^14.2.10",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-jss": "^10.9.2",
    "react-redux": "^7.2.9",
    "react-router-dom": "^5.3.4",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@mdx-js/loader": "^2.1.3",
    "@next/env": "^12.3.1",
    "chokidar": "^3.5.3",
    "codesandbox": "^2.2.3",
    "concurrently": "^7.4.0",
    "eslint-config-next": "^12.3.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.1.0",
    "glob": "10.3.3",
    "jsdoc": "^3.6.11",
    "jsdoc3-parser": "^2.0.0",
    "next-transpile-modules": "^8.0.0",
    "raw-loader": "^4.0.2",
    "react-docgen": "^5.4.3",
    "remark-gfm": "^3.0.1",
    "styled-jsx": "^5.0.7",
    "typedoc": "^0.24.8",
    "typescript": "^5.0.0",
    "url-loader": "^4.1.1"
  }
}
