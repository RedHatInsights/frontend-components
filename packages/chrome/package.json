  },
  "peerDependencies": {
    "@scalprum/react-core": "^0.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@redhat-cloud-services/types": "^3.0.0",
