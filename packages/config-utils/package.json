      "require": "./dist/proxy/index.js"
    },
    "./feo": {
      "types": "./dist/feo/index.d.ts",
      "import": "./dist/esm/feo/index.js",
      "require": "./dist/feo/index.js"
    },
    "./fec-logger": {
      "types": "./dist/fec-logger/index.d.ts",
      "import": "./dist/esm/fec-logger/index.js",
      "require": "./dist/fec-logger/index.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
