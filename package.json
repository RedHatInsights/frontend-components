{
  "name": "@red-hat-insights/insights-frontend-components",
  "version": "0.1.12",
  "main": "dist/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "index.css",
    "components/",
    "Utilities/"
  ],
  "dependencies": {
    "@patternfly/patternfly-next": "^1.0.3",
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.3",
    "patternfly-react": "^2.3.4",
    "d3": "^5.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^5.1.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js|jsx}",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "sassIncludes": {
    "patternfly": "node_modules/patternfly/dist/sass",
    "bootstrap": "node_modules/patternfly/node_modules/bootstrap-sass/assets/stylesheets",
    "fontAwesome": "node_modules/patternfly/node_modules/font-awesome-sass/assets/stylesheets"
  },
  "devDependencies": {
    "@patternfly/patternfly-next": "^1.0.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-fetch": "^2.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.0.4",
    "jest": "^23.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.22.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "topojson": "^3.0.2",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4",
    "webpack-visualizer-plugin": "^0.1.11",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "scripts": {
    "start": "npm run build",
    "build": "webpack --config config/webpack-build.config.js",
    "test": "jest",
    "prepublish": "rm -rf ./dist && npm run build"
  }
